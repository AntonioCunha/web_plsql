{
  "name": "web_plsql",
  "version": "0.2.1",
  "author": "Dieter Oberkofler <dieter.oberkofler@gmail.com>",
  "description": "The Express Middleware for Oracle PL/SQL",
  "keywords": [
    "node",
    "nodejs",
    "express",
    "middleware",
    "mod_plsql",
    "plsql",
    "pl/sql",
    "oracle",
    "ohs",
    "ords",
    "gateway",
    "apex"
  ],
  "homepage": "https://github.com/doberkofler/web_plsql",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "maintainers": [
    {
      "name": "Dieter Oberkofler",
      "email": "dieter.oberkofler@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/doberkofler/web_plsql/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/doberkofler/web_plsql.git"
  },
  "scripts": {
    "clean": "rimraf access.log lib trace coverage .nyc_output",
    "build": "tsc --project ./tsconfig.src.json",
    "eslint": "eslint --cache --report-unused-disable-directives \"./**/*.ts\"",
    "type-check": "tsc --project ./tsconfig.json --noEmit",
    "lint": "npm run eslint && npm run type-check",
    "test": "mocha",
    "test-cov": "npm run build && nyc mocha",
    "coveralls": "npm run test-cov && cat ./coverage/lcov.info | coveralls",
    "sample": "node examples/sample.js",
    "rebuild": "npm run clean && npm run lint && npm run build && npm run test",
    "create-package": "shx rm -f *.tgz && npm pack",
    "install": "npm run build"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "dependencies": {
    "@types/body-parser": "1.17.0",
    "@types/chai": "4.2.0",
    "@types/compression": "1.0.0",
    "@types/cookie-parser": "1.4.1",
    "@types/escape-html": "0.0.20",
    "@types/express": "4.17.0",
    "@types/mkdirp": "0.5.2",
    "@types/mocha": "5.2.7",
    "@types/morgan": "1.7.36",
    "@types/node": "12.7.1",
    "@types/oracledb": "1.11.34",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "body-parser": "1.19.0",
    "chai": "4.2.0",
    "compression": "1.7.4",
    "connect-multiparty": "2.2.0",
    "cookie-parser": "1.4.4",
    "coveralls": "3.0.6",
    "escape-html": "1.0.3",
    "eslint": "6.1.0",
    "express": "4.17.1",
    "express-status-monitor": "1.2.6",
    "http-parser-js": "0.5.1",
    "istanbul": "1.1.0-alpha.1",
    "mkdirp": "0.5.1",
    "mocha": "6.2.0",
    "morgan": "1.9.1",
    "nyc": "14.1.1",
    "rimraf": "3.0.0",
    "shx": "0.3.2",
    "source-map-support": "0.5.13",
    "supertest": "4.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "optionalDependencies": {
    "oracledb": "3.1.2"
  }
}
