{
  "name": "web_plsql",
  "version": "0.1.0",
  "author": "Dieter Oberkofler <dieter.oberkofler@gmail.com>",
  "description": "The Express Middleware for Oracle PL/SQL",
  "keywords": [
    "node",
    "nodejs",
    "express",
    "middleware",
    "mod_plsql",
    "plsql",
    "pl/sql",
    "oracle",
    "ohs",
    "ords",
    "gateway",
    "apex"
  ],
  "homepage": "https://github.com/doberkofler/web_plsql",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "maintainers": [
    {
      "name": "Dieter Oberkofler",
      "email": "dieter.oberkofler@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/doberkofler/web_plsql/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/doberkofler/web_plsql.git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel --out-dir=lib src",
    "flow-typed": "flow-typed install",
    "eslint": "eslint --report-unused-disable-directives src examples",
    "lint": "eslint --report-unused-disable-directives src examples && flow check --show-all-errors",
    "test": "npm run build && mocha --check-leaks --full-trace --require @babel/register test",
    "test-cov": "npm run build && istanbul cover node_modules/mocha/bin/_mocha -- --require @babel/register --colors --reporter dot test",
    "coveralls": "npm run build && npm run test-cov && cat ./coverage/lcov.info | coveralls",
    "sample": "node examples/sample.js",
    "start": "nodemon --watch ./lib --watch examples examples/sample.js",
    "watch": "babel --source-maps --watch --out-dir=lib src",
    "rebuild": "npm run clean && npm run lint && npm run build && npm run test",
    "create-package": "shx rm -f *.tgz && npm pack",
    "install": "npm run build"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-multiparty": "^2.1.1",
    "cookie-parser": "^1.4.1",
    "escape-html": "^1.0.3",
    "express": "^4.16.3",
    "express-status-monitor": "^1.1.5",
    "http-parser-js": "^0.4.13",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.0",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "eslint": "^5.5.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "flow-bin": "^0.80.0",
    "flow-typed": "^2.5.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "shx": "^0.3.2",
    "supertest": "^3.1.0"
  },
  "optionalDependencies": {
    "oracledb": "^2.3.0"
  }
}
