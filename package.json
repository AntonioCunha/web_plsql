{
  "name": "web_plsql",
  "author": "Dieter Oberkofler <dieter.oberkofler@gmail.com>",
  "description": "The Express Middleware for Oracle PL/SQL",
  "keywords": [
    "node",
    "nodejs",
    "express",
    "middleware",
    "mod_plsql",
    "plsql",
    "pl/sql",
    "oracle",
    "ohs",
    "ords",
    "apex"
  ],
  "homepage": "https://github.com/doberkofler/web_plsql",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "maintainers": [
    {
      "name": "Dieter Oberkofler",
      "email": "dieter.oberkofler@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/doberkofler/web_plsql/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/doberkofler/web_plsql.git"
  },
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && babel --out-dir=./dist ./src ./examples",
    "flow-typed": "flow-typed install",
    "lint": "eslint --report-unused-disable-directives ./src ./examples && flow check --show-all-errors",
    "test": "npm run clean && npm run build && mocha",
    "watch-run": "nodemon --watch ./dist ./dist/sample.js",
    "watch-run-lj": "nodemon --watch ./dist ./dist/lj.js",
    "watch-build": "babel --source-maps --watch --out-dir=./dist ./src ./examples"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "basic-auth": "^2.0.0",
    "body-parser": "^1.18.2",
    "colors": "^1.1.2",
    "compression": "^1.7.2",
    "connect-multiparty": "^2.1.0",
    "cookie-parser": "^1.4.1",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "http-parser-js": "^0.4.10",
    "morgan": "^1.9.0",
	"rimraf": "^2.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.18.2",
    "eslint-plugin-flowtype": "^2.46.1",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.3.0",
    "mocha": "^5.0.2",
    "nodemon": "^1.17.1"
  },
  "optionalDependencies": {
    "oracledb": "^2.1.2"
  }
}
