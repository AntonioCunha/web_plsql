{
  "name": "web_plsql",
  "version": "0.0.3",
  "author": "Dieter Oberkofler <dieter.oberkofler@gmail.com>",
  "description": "The Express Middleware for Oracle PL/SQL",
  "keywords": [
    "node",
    "nodejs",
    "express",
    "middleware",
    "mod_plsql",
    "plsql",
    "pl/sql",
    "oracle",
    "ohs",
    "ords",
    "gateway",
    "apex"
  ],
  "homepage": "https://github.com/doberkofler/web_plsql",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "maintainers": [
    {
      "name": "Dieter Oberkofler",
      "email": "dieter.oberkofler@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/doberkofler/web_plsql/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/doberkofler/web_plsql.git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel --out-dir=lib src",
    "flow-typed": "flow-typed install",
    "eslint": "eslint --report-unused-disable-directives src examples",
    "lint": "eslint --report-unused-disable-directives src examples && flow check --show-all-errors",
    "test": "mocha --check-leaks --full-trace --require babel-core/register test",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --require babel-core/register --colors --reporter dot test",
    "coveralls": "npm run test-cov && cat ./coverage/lcov.info | coveralls",
    "sample": "node examples/sample.js",
    "start": "nodemon --watch ./lib --watch examples examples/sample.js",
    "watch": "babel --source-maps --watch --out-dir=lib src",
    "rebuild": "npm run clean && npm run lint && npm run build && npm run test",
    "create-package": "shx rm -f *.tgz && npm pack",
    "install": "npm run build"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "connect-multiparty": "^2.1.0",
    "cookie-parser": "^1.4.1",
    "escape-html": "^1.0.3",
    "express": "^4.16.3",
    "http-parser-js": "^0.4.11",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.0",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "flow-bin": "^0.68.0",
    "flow-typed": "^2.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.2",
    "shx": "^0.2.2",
    "supertest": "^3.0.0"
  },
  "optionalDependencies": {
    "oracledb": "^2.1.2"
  }
}
