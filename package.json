{
  "name": "web_plsql",
  "version": "0.1.1",
  "author": "Dieter Oberkofler <dieter.oberkofler@gmail.com>",
  "description": "The Express Middleware for Oracle PL/SQL",
  "keywords": [
    "node",
    "nodejs",
    "express",
    "middleware",
    "mod_plsql",
    "plsql",
    "pl/sql",
    "oracle",
    "ohs",
    "ords",
    "gateway",
    "apex"
  ],
  "homepage": "https://github.com/doberkofler/web_plsql",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "maintainers": [
    {
      "name": "Dieter Oberkofler",
      "email": "dieter.oberkofler@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/doberkofler/web_plsql/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/doberkofler/web_plsql.git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc --project ./tsconfig.src.json",
    "eslint": "eslint --cache --report-unused-disable-directives \"./**/*.ts\"",
    "type-check": "tsc --project ./tsconfig.json --noEmit",
    "lint": "npm run eslint && npm run type-check",
    "test": "mocha",
    "test-cov": "npm run build && nyc mocha",
    "coveralls": "npm run test-cov && cat ./coverage/lcov.info | coveralls",
    "sample": "node examples/sample.js",
    "rebuild": "npm run clean && npm run lint && npm run build && npm run test",
    "create-package": "shx rm -f *.tgz && npm pack",
    "install": "npm run build"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "connect-multiparty": "2.2.0",
    "cookie-parser": "1.4.3",
    "escape-html": "1.0.3",
    "express": "4.16.4",
    "express-status-monitor": "1.2.3",
    "http-parser-js": "0.5.0",
    "mkdirp": "0.5.1",
    "morgan": "1.9.1",
    "rimraf": "2.6.3",
    "typescript": "3.2.4"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/chai": "4.1.7",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "1.4.1",
    "@types/escape-html": "0.0.20",
    "@types/express": "4.16.1",
    "@types/mkdirp": "0.5.2",
    "@types/mocha": "5.2.5",
    "@types/morgan": "1.7.35",
    "@types/node": "10.12.21",
    "@types/oracledb": "1.11.34",
    "@typescript-eslint/eslint-plugin": "1.2.0",
    "@typescript-eslint/parser": "1.2.0",
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "eslint": "5.13.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "5.2.0",
    "nodemon": "1.18.9",
    "nyc": "13.2.0",
    "shx": "0.3.2",
    "source-map-support": "0.5.10",
    "supertest": "3.4.2",
    "ts-node": "8.0.2"
  },
  "optionalDependencies": {
    "oracledb": "3.1.1"
  }
}
