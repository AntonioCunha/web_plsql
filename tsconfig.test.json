{
  "compilerOptions": {
    "skipLibCheck": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "outDir": "lib",
    "target": "es6",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "es2018"
    ],
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "alwaysStrict": true,
    "esModuleInterop": true
  }
}
